public class SudokuBoard {
	static int[][] grid;
	
	public SudokuBoard(){
		grid = new int[9][9];
	}
	
	public static void loadBoard1(){
		grid[0] = {0,4,0,0,8,0,0,0,0};
		grid[1] = {0,4,0,0,8,0,0,0,0};
		grid[2] = {0,4,0,0,8,0,0,0,0};
		grid[3] = {0,4,0,0,8,0,0,0,0};
		grid[4] = {0,4,0,0,8,0,0,0,0};
		grid[5] = {0,4,0,0,8,0,0,0,0};
		grid[6] = {0,4,0,0,8,0,0,0,0};
		grid[7] = {0,4,0,0,8,0,0,0,0};
		grid[8] = {0,4,0,0,8,0,0,0,0};		
	}

	public static void main(String[] args) {
		SudokuBoard sb = new SudokuBoard();
	}
	
}
