
public class StringReverser {


	public String frontBack(String str) {
		char[] c = str.toCharArray();
		int length = c.length;
		for(int i = 0; i < length/2; i++){
			char temp = c[i];
			c[i] = c[length-i-1];
			c[length-i] = temp;  
		}
		return new String(c);
	}


	public static void main(String[] args){
System.out.println("hey");
	}
}
